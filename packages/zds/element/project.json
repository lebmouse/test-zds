{
  "root": "packages/zds/element",
  "sourceRoot": "packages/zds/element/src",
  "projectType": "library",
  "tags": ["core"],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/zds/element",
        "tsConfig": "packages/zds/element/tsconfig.lib.json",
        "project": "packages/zds/element/package.json",
        "entryFile": "packages/zds/element/src/index.ts",
        "external": ["react/jsx-runtime", "@emotion/styled/base"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/zds/element/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/zds/element/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/zds/element"],
      "options": {
        "jestConfig": "packages/zds/element/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
