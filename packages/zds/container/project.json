{
  "root": "packages/zds/container",
  "sourceRoot": "packages/zds/container/src",
  "projectType": "library",
  "tags": ["core"],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/zds/container",
        "tsConfig": "packages/zds/container/tsconfig.lib.json",
        "project": "packages/zds/container/package.json",
        "entryFile": "packages/zds/container/src/index.ts",
        "external": ["react/jsx-runtime", "@emotion/styled/base"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/zds/container/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/zds/container/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/zds/container"],
      "options": {
        "jestConfig": "packages/zds/container/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
