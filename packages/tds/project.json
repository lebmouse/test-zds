{
  "projectType": "library",
  "root": "packages/tds",
  "sourceRoot": "packages/tds/src",
  "prefix": "zds",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/packages/tds"],
      "options": {
        "project": "packages/tds/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/tds/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/tds/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/tds"],
      "options": {
        "jestConfig": "packages/tds/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/tds/**/*.ts",
          "packages/tds/**/*.html",
          "packages/tds/core/**/*.ts",
          "packages/tds/core/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
